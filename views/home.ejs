      font-size: 1.1rem;
      border-radius: 20px;
      margin-right: 10px;
    }

    .btn-login {
      background-color: #dcdcdc;
      color: black;
      border: none;
    }

    .btn-register {
      background-color: #4CAF50;
      color: white;
      border: none;
    }

    /* Ajustar tamanho da logo */
    .logo img {
        width: 250px;
      }


    /* Estilos para info-section */
    .info-section {
      padding: 30px 0;
    }

    .info-section .container {
      display: flex;
      align-items: center;
      justify-content: space-between;
      flex-wrap: wrap;
    }

    .info-section .content {
      max-width: 50%;
      padding: 20px;
    }

    .info-section .content h2 {
      font-size: 2.5rem;
      font-weight: bold;
      color: #fff;
      margin-bottom: 20px;
    }

    .info-section .content p {
      font-size: 1rem;
      line-height: 1.6;
      margin-bottom: 20px;
      color: #EFECE4;
    }

    .info-section .content .stats {
      display: flex;
      gap: 30px;
      margin-bottom: 20px;
    }

    .info-section .content .stat-number {
      font-size: 2.5rem;
      font-weight: bold;
      color: #fff;
    }

    .info-section .content .stat-label {
      font-size: 1rem;
      color: #EFECE4;
    }

    .info-section .content .btn {
      padding: 12px 24px;
      font-size: 1rem;
      background: transparent;
      color: #fff;
      border: 2px solid #fff;
      border-radius: 30px;
      text-decoration: none;
      display: inline-block;
      transition: background 0.3s ease, color 0.3s ease;
    }

    .info-section .content .btn:hover {
      background: #EFECE4;
      color: #0c0c0c;
    }

    .info-section .image-container {
      max-width: 40%;
      padding: 20px;
    }

    .info-section .image-container img {
      width: 100%;
      border-radius: 20px;
      object-fit: cover;
    }

    @media (max-width: 768px) {
      .info-section .container {
        flex-direction: column;
      }

      .info-section .content, .info-section .image-container {
        max-width: 100%;
        text-align: center;
      }

      .info-section .content h2 {
        font-size: 2rem;
      }

      .info-section .content .stats {
        flex-direction: column;
        gap: 10px;
      }
    }

    /* Estilos para dispositivos móveis */
    @media (max-width: 768px) {
      .hero-section {
        background: url('/public/bgEnergIA.jpg') right/cover no-repeat, #0c0c0c;
