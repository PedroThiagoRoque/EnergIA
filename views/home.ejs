      display: flex;
      flex-direction: column;
      justify-content: center;
      height: 100vh;
      padding-top: 5%;
      padding-bottom: 5%;
      border-radius: 20px; /* Ajuste o valor conforme necessário para arredondar os cantos */
    }

    .custom-container {
      position: relative;
      width: 80%;
      max-width: 1200px;
      margin: auto;
      border: 2px solid white;
      border-radius: 20px;
      background-color: rgba(255, 255, 255, 0); /* Fundo transparente */
      padding: 30px;
      min-height: 80vh;
      display: flex;
      flex-direction: column;
      justify-content: space-between;
      color: white;
      
    }

      /* Estilo dos botões */
      .btn-login, .btn-register {
